config_version=4

[application]

config/name="Pong Neon"
config/description="A simple remix of the famous pong game with a cool neon effect"
run/main_scene="res://scenes/main_menu.tscn"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icons/l_icon.png"
config/macos_native_icon="res://icons/m_icon.icns"
config/windows_native_icon="res://icons/w_icon.ico"

[autoload]

SceneManager="*res://global/SceneManager.gd"

[debug]

gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/resizable=false
window/dpi/allow_hidpi=true

[global]

scripts=false
ignora=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

